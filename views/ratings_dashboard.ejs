<h1>Course Ratings</h1>

Test Success

<div>
    <% reviews.forEach(function(review){ %>
        <div class="col">
            <!--
                BOOTSTRAP CARD
                https://getbootstrap.com/docs/5.0/components/card/
            -->
            <div class="card" style="margin-bottom: 2em;">
                <div class="card-body">
                    <h5 class="card-title">Course Name: <%= review["course_name"] %></h5>
                    <p class="card-text">Course Difficulty: <%= review["difficulty"] %></p>
                    <p class="card-text">Amount of Assessments: <%= review["assessments"] %></p>
                    <p class="card-text">Amount of Group Work: <%= review["groupWork"] %></p>

                </div>
            </div>
        </div>
    <% }) %>
</div>